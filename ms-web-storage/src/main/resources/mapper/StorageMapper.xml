<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pwd.initializr.storage.persistence.mapper.StorageMapper">
  <!-- insertFile -->
  <insert id="insertFile" keyProperty="id"
    parameterType="pwd.initializr.storage.persistence.dao.StorageEntity"
    useGeneratedKeys="true">
		insert into `storage`
		(`id`,`user_id`,`name`,`url`,`path`,`status`,`create_time`,`update_time`)
		values
		(#{id},#{userId},#{name},#{url},#{path},#{status},#{createTime},#{updateTime})
	</insert>

</mapper>