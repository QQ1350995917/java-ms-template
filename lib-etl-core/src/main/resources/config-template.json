{
  "group": "group",
  "name": "name",
  "id": "id",
  "logDir": "",
  "input": {
    "capacity": 100,
    "scanners": [
      {
        "name": "name",
        "type": "file",
        "id": "file",
        "enable": true,
        "sourceDir": "file:///",
        "suffix": "txt",
        "completeSuffix": "ok",
        "capacity": 5,
        "processor": {
          "class": "pwd.initializr.etl.core.input.processor.TXTProcessor",
          "charset": "utf-8",
          "delimiter": "\r\n",
          "threadNum": 3,
          "over": {
            "strategy": "MOVE",
            "targetDir": "file:///"
          }
        }
      },
      {
        "name": "name",
        "type": "file",
        "id": "file",
        "enable": true,
        "sourceDir": "file:///",
        "suffix": "zip",
        "completeSuffix": "ok",
        "capacity": 5,
        "processor": {
          "class": "pwd.initializr.etl.core.input.processor.ZIPProcessor",
          "charset": "utf-8",
          "delimiter": "\r\n",
          "threadNum": 3,
          "over": {
            "strategy": "MOVE",
            "targetDir": "file:///"
          }
        }
      },
      {
        "name": "name",
        "type": "kafka",
        "id": "kafka",
        "enable": true,
        "bootstrapServers": "192.168.123.128:9092",
        "groupId": "groupID",
        "enableAutoCommit": true,
        "autoCommitIntervalMs": 1000,
        "autoOffsetReset": "earliest",
        "sessionTimeoutMs": 30000,
        "keyDeserializer": "org.apache.kafka.common.serialization.StringDeserializer",
        "valueDeserializer": "org.apache.kafka.common.serialization.StringDeserializer",
        "topic": "topic",
        "processor": {
          "class": "pwd.initializr.etl.core.input.processor.KAFKAProcessor",
          "threadNum": 3
        }
      }
    ]
  },
  "handle": {
    "capacity": 100,
    "threadNum": 1,
    "handlers": [
      {
        "enable": true,
        "class": "pwd.initializr.etl.core.plugin.handle.LineSplitHandler",
        "delimiter": "\\|",
        "fields": [
          "key0",
          "key1",
          "key2",
          "key3",
          "key4",
          "key5",
          "key6"
        ]
      }
    ]
  },
  "output": {
    "outputs": [
      {
        "name": "name",
        "type": "kafka",
        "id": "kafka",
        "enable": true,
        "threadNum": 1,
        "bootstrapServers": "192.168.123.128:9092",
        "acks": "1",
        "retries": 0,
        "compressionType": "snappy",
        "batchSize": 100,
        "lingerMs": 100,
        "bufferMemory": 10240,
        "maxInFlightRequestsPerConnection": 1,
        "keyDeserializer": "org.apache.kafka.common.serialization.StringDeserializer",
        "valueDeserializer": "org.apache.kafka.common.serialization.StringDeserializer",
        "topic": "topic-out"
      }
    ]
  }
}
