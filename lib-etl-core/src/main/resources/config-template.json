{
  "group": "group",
  "name": "name",
  "id": "id",
  "logDir": "",
  "input": {
    "capacity": 100,
    "scanners": [
      {
        "name": "name",
        "type": "file",
        "enable": true,
        "sourceDir": "file:///",
        "suffix": "txt",
        "completeSuffix": "ok",
        "capacity": 5,
        "processor": {
          "class": "pwd.initializr.etl.core.input.processor.TXTProcessor",
          "charset":"utf-8",
          "delimiter": "\r\n",
          "threadNum": 3,
          "over": {
            "strategy": "MOVE",
            "targetDir": "file:///"
          }
        }
      },
      {
        "name": "name",
        "type": "file",
        "enable": true,
        "sourceDir": "file:///",
        "suffix": "zip",
        "completeSuffix": "ok",
        "capacity": 5,
        "processor": {
          "class": "pwd.initializr.etl.core.input.processor.ZIPProcessor",
          "charset":"utf-8",
          "delimiter": "\r\n",
          "threadNum": 3,
          "over": {
            "strategy": "MOVE",
            "targetDir": "file:///"
          }
        }
      },
      {
        "name": "name",
        "type": "kafka",
        "enable": true,
        "bootstrapServers": "192.168.123.128:9092",
        "groupId": "groupID",
        "enableAutoCommit": true,
        "autoCommitIntervalMs": 1000,
        "autoOffsetReset": "earliest",
        "sessionTimeoutMs": 30000,
        "keyDeserializer": "org.apache.kafka.common.serialization.StringDeserializer",
        "valueDeserializer": "org.apache.kafka.common.serialization.StringDeserializer",
        "topic": "topic",
        "processor": {
          "class": "pwd.initializr.etl.core.input.processor.KAFKAProcessor",
          "threadNum": 3
        }
      }
    ]
  }
}
