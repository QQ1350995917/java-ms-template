{
  "group": "group",
  "name": "name",
  "id": "id",
  "logDir": "",
  "input": {
    "capacity": 10,
    "scanners": [
      {
        "name": "name",
        "type": "file",
        "enable": false,
        "sourceDir": "/Users/pwd/Documents/etl/input",
        "suffix": "txt",
        "completeSuffix": "ok",
        "capacity": 5,
        "processor": {
          "class": "pwd.initializr.etl.core.input.processor.TXTProcessor",
          "charset":"utf-8",
          "rowDelimiter": "\r\n",
          "columnDelimiter": "\\|",
          "threadNum": 4,
          "over": {
            "strategy": "Delete",
            "targetDir": "/Users/pwd/Documents/etl/output"
          }
        }
      },
      {
        "name": "name",
        "type": "kafka",
        "enable": true,
        "bootstrapServers": "127.0.0.1:9092",
        "groupId": "testGroupID",
        "enableAutoCommit": true,
        "autoCommitIntervalMs": 1000,
        "autoOffsetReset": "earliest",
        "sessionTimeoutMs": 30000,
        "keyDeserializer": "org.apache.kafka.common.serialization.StringDeserializer",
        "valueDeserializer": "org.apache.kafka.common.serialization.StringDeserializer",
        "topic": "etl-test",
        "processor": {
          "class": "pwd.initializr.etl.core.input.processor.KAFKAProcessor",
          "threadNum": 3
        }
      }
    ]
  },
  "handle": {
    "capacity": 100,
    "threadNum": 1,
    "handlers": [
      {
        "enable": true,
        "class": "pwd.initializr.etl.core.plugin.handle.LineSplitHandler",
        "delimiter": "\\|",
        "fields": [
          "key0",
          "key1",
          "key2",
          "key3",
          "key4",
          "key5",
          "key6"
        ]
      }
    ]
  },
  "output": {
    "outputs": [
      {
        "name": "name",
        "type": "kafka",
        "id": "kafka",
        "enable": true,
        "threadNum": 1,
        "bootstrapServers": "127.0.0.1:9092",
        "acks": "1",
        "retries": 0,
        "compressionType": "snappy",
        "batchSize": 100,
        "lingerMs": 100,
        "bufferMemory": 10240,
        "maxInFlightRequestsPerConnection": 1,
        "keySerializer": "org.apache.kafka.common.serialization.StringSerializer",
        "valueSerializer": "org.apache.kafka.common.serialization.StringSerializer",
        "topic": "etl-test-out"
      }
    ]
  }
}
